<div class="lab-edit">
  <%= form_with model: @laboratory, url: path, id: 'edit_laboratory', class: 'edit_laboratory', local: true, method: :patch do |f| %>
    <div class="lab-edit-form">
      <h3 class="form-title-name">About</h3>
      <%= f.submit "登録", class:"btn btn-primary btn-sm"%>
    </div>
    
    <%= f.text_area :introduction %>
    <%= f.hidden_field :laboratory_id, :value => @set_laboratory.id %>
    
  <% end %>
</div>