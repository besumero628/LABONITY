<!--Confernce Form-->
<div class="row form-title">
  <h3 class="form-title-name col-12">Link <%= title %></h3>
</div>

<div class="form-main">
  <%= form_with model: @lablink, url: path, id: 'new_lablink', class: 'new_lablink', local: true do |f| %>
    <%= markup do |m|
      p = FormPresenter.new(f, self)

      m << p.text_field_block(:name, "リンク名", {placeholder: ""})
      m << p.text_field_block(:linkpath, "Path", {placeholder: "http://www..."})

    end %>

    <%= f.hidden_field :laboratory_id, :value => @set_laboratory.id %>

  <div class="form-main-submit">
    <%= f.submit "登録", class:"btn btn-primary col-6"%>
  </div>

  <% end %>
</div>

<div class="row other-link">
  <%= link_to "Link一覧に戻る", public_laboratory_lablinks_path(@set_laboratory.id), class:"col-12" %>
</div>